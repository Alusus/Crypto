import "Srl/Console";
import "Apm";
Apm.importFile("Alusus/Crypto");
use Srl;
def text: String("Crypto binding for Alusus");
Console.print("String : %s\n", text.buf);
Console.print("md5 String : %s\n", Crypto.md5(text).buf);
Console.print("base64 encode String : %s\n", Crypto.encodeBase64(text).buf);
Console.print("base 64 decode String : %s\n", Crypto.decodeBase64(Crypto.encodeBase64(text)).buf);
